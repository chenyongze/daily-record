{
  "title": "yongze.chen daily record",
  "description": "daily record",
  "author": "yongze.chen",
  "output.name": "site",
  "language": "zh-hans",
  "direction": "rtl",
  "gitbook": "3.2.3",
  "root": ".",
  "style": {
    "website": "styles/website.css"
  },
  "structure": {
    "readme": "README.md"
  },
  "links": {
    "sidebar": {
      "首页": "http://daily.dotalk.cn"
    }
  },
  "plugins": [
    "tbfed-pagefooter",
    "donate",
    "copy-code-button",
    "alerts",
