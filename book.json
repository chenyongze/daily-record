    "expandable-chapters",
    "copy-code-button",
    "todo",
    "advanced-emoji",
    "github",
    "splitter",
    "sharing-plus",
    "lightbox",
    "summary"
  ],
  "pluginsConfig": {
    "sharing": {
      "douban": false,
      "facebook": false,
      "google": true,
      "pocket": false,
      "qq": false,
      "qzone": false,
      "twitter": false,
      "weibo": true,
      "all": [
        "douban",
        "facebook",
        "google",
        "twitter",
        "weibo",
        "qq",
        "whatsapp"
      ]
    },
    "tbfed-pagefooter": {
      "copyright": "&copy yongze.chen flag:锻炼、健身、读书",
      "modify_label": "文件修订时间：",
      "modify_format": "YYYY-MM-DD HH:mm:ss"
    },
    "chapter-fold": {},
    "hide-element": {
      "elements": [
        ".gitbook-link",
        ".copyright"
      ]
    },
    "donate": {
      "wechat": "http://static.dotalk.cn/daily/pay/wechat.jpeg",
      "alipay": "http://static.dotalk.cn/daily/pay/alipay.jpeg",
      "title": "",
      "button": "赏",
      "alipayText": "支付宝打赏",
      "wechatText": "微信打赏"
    },
    "code": {
      "copyButtons": false
    },
    "rss": {
      "title": "GitBook 使用教程",
      "description": "记录 GitBook 的配置和一些插件的使用",
      "author": "yongze.chen",
      "feed_url": "http://daily.dotalk.cn/rss",
      "site_url": "http://daily.dotalk.cn/",
      "managingEditor": "",
      "webMaster": "",
      "categories": [
        "gitbook"
      ]
    },
    "github": {
      "url": "https://github.com/chenyongze"
    },
    "github-buttons": {
      "repo": "chenyongze/daily-record",
      "types": [
        "star",
        "watch",
        "fork"
      ],
      "size": "small"
    },
    "baidu-tongji": {
      "token": "55e7dfe47f4dc1c12345678fdfa62565"
    },
    "theme-default": {
      "showLevel": false
    },
    "anchor-navigation-ex": {
      "showLevel": false,
      "associatedWithSummary": true,
      "printLog": true,
      "multipleH1": true,
      "mode": "float",
      "float": {
        "showLevelIcon": true,
        "level1Icon": "fa fa-hand-o-right",
        "level2Icon": "fa fa-hand-o-right",
        "level3Icon": "fa fa-hand-o-right",
        "level4Icon": "fa fa-hand-o-right"
      }
    }
